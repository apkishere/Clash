function HOME()
bypass = gg.choice({"🔓​ေက်ာ္​မယ္​🔓","⚠️ခိုးမယ္⚠️","ထြက္​မယ္​"},nail,"​ေက်ာ္​ဖို႔မ​ေမ့နဲ႔")
if bypass == 1 then pass() end
if bypass == 2 then no() end
if bypass == 3 then Exit() end
HOMEDM= -1
end

function pass()
