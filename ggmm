function HOME()
bypass = gg.choice({"ğŸ”“â€‹á€±á€€á€ºá€¬á€¹â€‹á€™á€šá€¹â€‹ğŸ”“","âš ï¸á€á€­á€¯á€¸á€™á€šá€¹âš ï¸","á€‘á€¼á€€á€¹â€‹á€™á€šá€¹â€‹"},nail,"â€‹á€±á€€á€ºá€¬á€¹â€‹á€–á€­á€¯á‚”á€™â€‹á€±á€™á€·á€”á€²á‚”")
if bypass == 1 then pass() end
if bypass == 2 then no() end
if bypass == 3 then Exit() end
HOMEDM= -1
end

